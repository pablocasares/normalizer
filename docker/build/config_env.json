{
  "application.id": "${APPLICATION_ID}",
  "bootstrap.servers": "${KAFKA_BOOTSTRAP_SERVER}",
  "num.stream.threads": ${NUM_STREAM_THREADS},
  "bootstrapper.classname": "${BOOTSTRAP_CLASSNAME}",
  "file.bootstrapper.path": "${BOOTSTRAP_FILEPATH}",
  "metric.enable": ${METRIC_ENABLE},
  "metric.kafka.topic": "${METRIC_KAFKA_TOPIC}",
  "metric.listeners": ["io.wizzie.metrics.listeners.KafkaMetricListener"],
  "metric.interval": ${METRIC_INTERVAL},
  "metric.databag": {"component":"normalizer"},
  "multi.id": ${MULTI_ID},
  "max.output.kafka.topics": ${MAX_OUTPUT_KAFKA_TOPICS},
  "bootstrap.kafka.topics": ${BOOTSTRAP_KAFKA_TOPICS},
  "default.timestamp.extractor":"org.apache.kafka.streams.processor.WallclockTimestampExtractor",
  "producer.buffer.memory": 102400,
  "consumer.fetch.max.bytes": 102400,
  "consumer.max.poll.records": 50,
  "consumer.fetch.max.wait.ms": 500,
  "consumer.send.buffer.bytes": 131072,
  "consumer.receive.buffer.bytes": 65536,
  "producer.send.buffer.bytes": 131072,
  "producer.receive.buffer.bytes": 32768,
  "buffered.records.per.partition": 500
}
